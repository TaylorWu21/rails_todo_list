<h1>To Do List</h1>
<p><%= link_to 'New Item', new_item_path %></p>
<% @items.each do |item| %>
	<h2><%= item.name %></h2>
	<p><%= item.description %></p>
	<p><%= item.complete %></p>
	<%= link_to 'Show To do', item_path(item) %>
	<%= link_to 'Edit', edit_item_path(item) %>
	<%= link_to 'Delete', item_path(item) , method: :delete %>
<% end %>
